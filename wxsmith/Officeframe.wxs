<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="OfficeFrame">
		<size>400,357</size>
		<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
			<label>Welcome to the,</label>
			<pos>8,16</pos>
			<font>
				<size>10</size>
				<style>normal</style>
				<weight>bold</weight>
				<underlined>0</underlined>
				<family>default</family>
				<face>Arial</face>
			</font>
		</object>
		<object class="wxButton" name="ID_STUDENT" variable="student" member="yes">
			<label>Student</label>
			<pos>144,104</pos>
			<size>120,23</size>
			<font>
				<size>12</size>
				<style>italic</style>
				<weight>bold</weight>
				<underlined>0</underlined>
				<family>swiss</family>
				<face>Candara</face>
			</font>
			<handler function="OnstudentClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
			<label>OFFICE MANAGEMENT SYSTEM</label>
			<pos>48,40</pos>
			<size>288,32</size>
			<font>
				<size>14</size>
				<style>normal</style>
				<weight>bold</weight>
				<underlined>0</underlined>
				<family>modern</family>
				<face>Lucida Console</face>
			</font>
		</object>
		<object class="wxButton" name="ID_TEACHER" variable="teacher" member="yes">
			<label>Teacher</label>
			<pos>144,160</pos>
			<size>120,23</size>
			<font>
				<size>12</size>
				<style>italic</style>
				<weight>bold</weight>
				<underlined>0</underlined>
				<family>swiss</family>
				<face>Candara</face>
			</font>
			<handler function="OnteacherClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxButton" name="ID_ADMIN" variable="admin" member="yes">
			<label>Administrator</label>
			<pos>144,216</pos>
			<size>120,23</size>
			<font>
				<size>12</size>
				<style>italic</style>
				<weight>bold</weight>
				<underlined>0</underlined>
				<family>swiss</family>
				<face>Candara</face>
			</font>
			<handler function="OnadminClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxButton" name="ID_EXIT" variable="exit" member="yes">
			<label>Exit</label>
			<pos>32,288</pos>
			<size>56,23</size>
			<bg>wxSYS_COLOUR_INACTIVECAPTION</bg>
			<font>
				<size>12</size>
				<style>normal</style>
				<weight>bold</weight>
				<underlined>0</underlined>
				<family>swiss</family>
				<face>Bahnschrift</face>
			</font>
			<handler function="OnexitClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
